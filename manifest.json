{
  "manifest_version": 2,
  "name": "Dota Pro Circuit",
  "description": "Unofficial port for Valve's Dota Pro Circuit app",
  "version": "1.0",
  "icons": {
    "128": "img/icon.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ],
      "pages": [
        "html/embed.html"
      ]
    }
  },
  "permissions": [
    "webview",
    "fullscreen",
    "notifications"
  ]
}
